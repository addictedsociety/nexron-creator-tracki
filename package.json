{
  "name": "web-monorepo",
  "private": true,
  "scripts": {
    "dev:fe": "npm --prefix ./frontend run dev -- --host",
    "dev:be": "npm --prefix ./backend run start:dev",
    "dev:all": "concurrently -n FE,BE -c auto \"npm:dev:fe\" \"npm:dev:be\"",
    "db:up": "docker compose -f docker/compose.db.yml up -d",
    "db:down": "docker compose -f docker/compose.db.yml down"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "dependencies": {
    "@clerk/themes": "^2.4.25",
    "@clerk/vue": "^1.14.1"
  }
}
